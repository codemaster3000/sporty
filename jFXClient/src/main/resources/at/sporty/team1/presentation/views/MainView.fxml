<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1"
			xmlns="http://javafx.com/javafx/8"
			fx:id="_borderPanel"
			fx:controller="at.sporty.team1.presentation.controllers.MainViewController">
	<top>
        <ToolBar BorderPane.alignment="CENTER">
            <Button mnemonicParsing="false" text="Add Member" onAction="#openNewMemberView"/>
			<Button mnemonicParsing="false" text="Test Team" onAction="#openNewTeamView"/>
        </ToolBar>
	</top>
	<center>
		<TabPane fx:id="_tabPanel" BorderPane.alignment="CENTER" tabMaxHeight="Infinity"/>
	</center>
</BorderPane>